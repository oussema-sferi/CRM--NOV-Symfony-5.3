                <div class="row">
                    <div class="col-xl-4 col-md-6">
                        <div class="card card-stats">
                            <!-- Card body -->
                            {% if app.session.get('date_filter_value_my_stats') is not null %}
                                {% set filter = app.session.get('date_filter_value_my_stats') %}
                                {% set selected_date = "now"|date_modify("-" ~ filter ~ " day")| date('Y-m-d') %}
                            {% endif%}
                            <div class="card-body">
                                <div class="row">
                                    <div class="col">
                                        <h5 class="card-title text-uppercase text-muted mb-0">Fiches de contact traités</h5>
                                        <span class="h2 font-weight-bold mb-0">
                                            {% if app.session.get('date_filter_value_my_stats') is not null %}
                                                {% set processed_clients_counter = 0 %}
                                                {% for client in processed_clients %}
                                                    {% set break = false %}
                                                    {% for call in client.calls %}
                                                        {% if break == false %}
                                                            {% set call_date = call.createdAt | date('Y-m-d') %}
                                                            {% if filter == "0" or filter == "1" %}
                                                                {% if call_date == selected_date %}
                                                                    {% set processed_clients_counter = processed_clients_counter + 1 %}
                                                                    {% set break = true %}
                                                                {% endif %}
                                                            {% elseif filter == "100" %}
                                                                {% if call_date >= start_date and call_date <= end_date %}
                                                                    {% set processed_clients_counter = processed_clients_counter + 1 %}
                                                                    {% set break = true %}
                                                                {% endif %}
                                                            {% else %}
                                                                {% if call_date >= selected_date %}
                                                                    {% set processed_clients_counter = processed_clients_counter + 1 %}
                                                                    {% set break = true %}
                                                                {% endif%}
                                                            {% endif %}
                                                        {% endif %}
                                                    {% endfor %}
                                                {% endfor %}
                                                {{ processed_clients_counter }}
                                            {% else %}
                                                {{ processed_clients_count }}
                                            {% endif %}
                                        </span>
                                    </div>
                                    <div class="col-auto">
                                        <div class="icon icon-shape bg-gradient-cyan text-white rounded-circle shadow">
                                            <i class="fas fa-user-friends"></i>
                                        </div>
                                    </div>
                                </div>
