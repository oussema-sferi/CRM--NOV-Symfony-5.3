                                <li class="breadcrumb-item active" aria-current="page">Mes Statistiques</li>
                                {#<li class="breadcrumb-item active" aria-current="page">Default</li>#}
                            </ol>
                        </nav>
                    </div>
                    <div class="col-lg-9 col-5 text-right">
                        {#<a href="#" class="btn btn-sm btn-neutral">Période</a>#}
                            <div class="form-group" id="normal_period_block">
                                <label for="period" style="color: #fff; font-weight: bold; font-family: 'Times New Roman'">Période : </label>
                                <select class="form-control-sm" name="normal_period" id="normal_period">
                                    <option value="" {% if app.session.get('date_filter_value_my_stats') is not defined or app.session.get('date_filter_value_my_stats') is null %} selected {% endif %}>Choisir...</option>
                                    <option value="0" {% if app.session.get('date_filter_value_my_stats') is defined and app.session.get('date_filter_value_my_stats') == "0" %} selected {% endif %}>Aujourd'hui</option>
                                    <option value="1" {% if app.session.get('date_filter_value_my_stats') is defined and app.session.get('date_filter_value_my_stats') == "1" %} selected {% endif %}>Hier</option>
                                    <option value="7" {% if app.session.get('date_filter_value_my_stats') is defined and app.session.get('date_filter_value_my_stats') == "7" %} selected {% endif %}>7 derniers jours</option>
                                    <option value="14" {% if app.session.get('date_filter_value_my_stats') is defined and app.session.get('date_filter_value_my_stats') == "14" %} selected {% endif %}>14 derniers jours</option>
                                    <option value="30" {% if app.session.get('date_filter_value_my_stats') is defined and app.session.get('date_filter_value_my_stats') == "30" %} selected {% endif %}>30 derniers jours</option>
                                    <option value="60" {% if app.session.get('date_filter_value_my_stats') is defined and app.session.get('date_filter_value_my_stats') == "60" %} selected {% endif %}>60 derniers jours</option>
                                    <option value="90" {% if app.session.get('date_filter_value_my_stats') is defined and app.session.get('date_filter_value_my_stats') == "90" %} selected {% endif %}>90 derniers jours</option>
                                    <option value="100" {% if app.session.get('date_filter_value_my_stats') is defined and app.session.get('date_filter_value_my_stats') == "100" %} selected {% endif %}>Personalisée</option>
                                </select>
                                <a href="{{ path('my_stats_filters_notifications') }}" class="btn btn-sm btn-success">Appliquer les filtres</a>
                                <a href="{{ path('my_stats_filters_initialization') }}" class="btn btn-sm btn-primary">Réinitialiser les filtres</a>
                            </div>

                            <form action="{{ path('all_stats_filters') }}" method="post">
                                <div class="form-group" id="custom_period_block" style="display: none">
                                    <input type="text" name="dateFilterValueAllStats" value="100" hidden>
                                    <label for="start_date" style="font-weight: bolder; font-family: Cambria; color: #fff">De: </label>
                                    <input type="date" class="form-control-sm" name="start_date" id="start_date" required>
                                    <label for="start_date" style="font-weight: bolder; font-family: Cambria; color: #fff">Jusqu'à: </label>
                                    <input type="date" class="form-control-sm" name="end_date" id="end_date" required>
                                    <a id="retour_period" class="btn btn-sm btn-neutral" style="margin-left: 10px">Retour</a>
                                    <input class="btn btn-sm btn-success" type="submit" value="Appliquer les filtres">
                                    <a href="{{ path('all_stats_filters_initialization') }}" class="btn btn-sm btn-primary" id="between-date-initialization" style="display: none">Réinitialiser les filtres</a>
                                </div>
